{
  "kind": "singleType",
  "collectionName": "home_pages",
  "info": {
    "singularName": "home-page",
    "pluralName": "home-pages",
    "displayName": "Home Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": false
    },
    "slug": {
      "type": "text",
      "required": false,
      "unique": true
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "sections.hero-slider",
        "sections.statistics-card-list",
        "sections.rich-text-image",
        "sections.icon-text-list-image",
        "sections.card-list",
        "sections.rockstar-info-list",
        "sections.banner-cards",
        "sections.banner",
        "sections.leaderboard",
        "sections.faq-section",
        "sections.richtext-card",
        "organisms.tab-list"
      ]
    },
    "layout": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::layout.layout"
    },
    "seo_section": {
      "type": "component",
      "repeatable": false,
      "component": "sections.seo"
    }
  }
}
