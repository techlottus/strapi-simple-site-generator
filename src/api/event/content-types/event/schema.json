{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "start_datetime": {
      "type": "datetime"
    },
    "end_datetime": {
      "type": "datetime"
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event-location.event-location",
      "inversedBy": "event"
    },
    "expositor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::expositor.expositor",
      "inversedBy": "event"
    },
    "text_location": {
      "type": "string"
    },
    "desktop_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "tablet_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "mobile_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "short_description": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price_list": {
      "type": "component",
      "repeatable": false,
      "component": "sections.prices"
    },
    "details": {
      "type": "richtext"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event-category.event-category",
      "inversedBy": "event"
    },
    "modality": {
      "type": "enumeration",
      "enum": [
        "online",
        "presencial"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "breadcrumb": {
      "type": "string"
    },
    "total_duration": {
      "type": "integer"
    },
    "attendant_list": {
      "type": "string"
    },
    "campus": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::campus.campus"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "active",
        "canceled",
        "finished"
      ]
    },
    "is_private": {
      "type": "boolean"
    },
    "recording": {
      "allowedTypes": [
        "videos"
      ],
      "type": "media",
      "multiple": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "sponsors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::brand.brand"
    },
    "agenda": {
      "displayName": "event_agenda",
      "type": "component",
      "repeatable": false,
      "component": "events.event-agenda"
    }
  }
}
