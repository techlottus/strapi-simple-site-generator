{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "expositors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::expositor.expositor",
      "inversedBy": "events"
    },
    "desktop_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "tablet_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "mobile_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "short_description": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price_list": {
      "type": "component",
      "repeatable": false,
      "component": "sections.prices"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event-category.event-category",
      "inversedBy": "events"
    },
    "modality": {
      "type": "enumeration",
      "enum": [
        "online",
        "presencial"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "breadcrumb": {
      "type": "string"
    },
    "total_duration": {
      "type": "integer"
    },
    "attendant_list": {
      "type": "string"
    },
    "campus": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::campus.campus"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "active",
        "canceled",
        "finished"
      ]
    },
    "is_private": {
      "type": "boolean"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "sponsors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::brand.brand"
    },
    "agenda": {
      "displayName": "event_agenda",
      "type": "component",
      "repeatable": false,
      "component": "events.event-agenda"
    },
    "contact_link": {
      "type": "component",
      "repeatable": false,
      "component": "sections.link"
    },
    "video": {
      "displayName": "youtube_video",
      "type": "component",
      "repeatable": false,
      "component": "events.youtube-video"
    },
    "metadata": {
      "type": "json"
    },
    "attendant_list_id": {
      "type": "integer"
    },
    "location": {
      "displayName": "event_location",
      "type": "component",
      "repeatable": false,
      "component": "events.event-location"
    },
    "start_date": {
      "type": "date",
      "required": true
    },
    "start_time": {
      "type": "time",
      "required": true,
      "default": "16:30"
    },
    "end_date": {
      "type": "date",
      "required": true
    },
    "end_time": {
      "type": "time",
      "required": true,
      "default": "19:00"
    },
    "start_register_date": {
      "type": "date",
      "required": true
    },
    "start_register_time": {
      "type": "time",
      "required": true,
      "default": "08:00"
    },
    "end_register_date": {
      "type": "date",
      "required": true
    },
    "end_register_time": {
      "type": "time",
      "required": true,
      "default": "18:00"
    },
    "details_json": {
      "type": "json"
    },
    "details": {
      "type": "richtext"
    }
  }
}
